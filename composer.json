{
    "name": "minkbear/adldap2-laravel",
    "description": "Adldap2 for Laravel 5.",
    "keywords": ["adldap", "adldap2", "ldap", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.4.16",
        "adldap2/adldap2": "5.2.*",
        "laravel/framework": "5.*"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "mockery/mockery": "0.9.*"
    },
    "archive": {
        "exclude": ["/tests"]
    },
    "autoload": {
        "psr-4": {
            "Adldap\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Adldap\\Laravel\\Tests\\": "tests/"
        }
    }
}
